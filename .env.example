# Oracle Autonomous Database Configuration
# Copy this file to .env and fill in your actual values

# Database Credentials
DB_USERNAME=ADMIN
DB_PASSWORD=your_admin_password_here

# Database Connection
# Service names: freestackdb_high, freestackdb_medium, freestackdb_low
# Replace 'freestackdb' with your actual database name
DB_SERVICE_NAME=freestackdb_high

# Wallet Location (relative to project root)
DB_WALLET_PATH=src/main/resources/wallet
